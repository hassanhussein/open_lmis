	<queryString language="json">
		<![CDATA[(SELECT DISTINCT facilities.code, facilities.name, gz.district_name as location, gz.region_name as province,
 p.name as program, ft.name as facilityType, 'REPORTED' as reportingStatus, period.name period, r.status as rnrStatus, r.id as rnrId,
  (select max(rs.createdDate) from requisition_status_changes rs where rs.rnrid = r.id and rs.status = 'AUTHORIZED') as createdDate
,'year' as year, 'month' as month
FROM facilities
INNER JOIN requisition_group_members rgm on rgm.facilityid = facilities.id
INNER JOIN vw_districts gz on gz.district_id = facilities.geographiczoneid
INNER JOIN facility_types ft on ft.id = facilities.typeid
INNER JOIN programs_supported ps on ps.facilityId = facilities.id and ps.programId = 3
INNER JOIN programs p on p.id = ps.programId
INNER JOIN processing_periods period on period.id = 109
INNER JOIN requisition_group_program_schedules rgps on rgps.requisitionGroupId = rgm.requisitionGroupId and ps.programId = rgps.programId
INNER JOIN requisitions r on r.facilityId = facilities.id and r.programId = ps.programId and r.periodId = period.id and r.emergency = false
 order by name
 )

union

SELECT DISTINCT facilities.code, facilities.name, gz.district_name as location, gz.region_name as province, p.name as program,
ft.name as facilityType, 'NON_REPORTING' as reportingStatus,period.name period, null as rnrStatus, r.id as rnrId,
 (select max(rs.createdDate) from requisition_status_changes rs where rs.rnrid = r.id and rs.status = 'AUTHORIZED') as createdDate
,'year' as year, 'month' as month
FROM facilities
INNER JOIN requisition_group_members rgm on rgm.facilityid = facilities.id
INNER JOIN vw_districts gz on gz.district_id = facilities.geographiczoneid
INNER JOIN facility_types ft on ft.id = facilities.typeid
INNER JOIN programs_supported ps on ps.facilityId = facilities.id and ps.programId = 3
INNER JOIN programs p on p.id = ps.programId
INNER JOIN processing_periods period on period.id = 109
INNER JOIN requisition_group_program_schedules rgps on rgps.requisitionGroupId = rgm.requisitionGroupId and ps.programId = rgps.programId
INNER JOIN requisitions r on r.facilityId = facilities.id and r.programId = ps.programId and r.periodId = period.id and r.emergency = false
 order by name;]]>
	</queryString>
