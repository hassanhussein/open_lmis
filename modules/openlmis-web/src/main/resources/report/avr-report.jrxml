    <queryString language="SQL">
        <![CDATA[select
r.country as hdr_country,
RECEIPTNUMBER as hdr_reportnumber,
to_char(now(), 'DD/MM/YYYY') as hdr_dateofreport,
'AAABBCC'||', '||to_char(r.receivedate, 'DD/MM/YYYY') as hdr_nameofcoldstore,
'CVS'||', '|| to_char(inspectionDate, 'DD/MM/YYYY') as hdr_completiontime,
i.id as ins_id,
i.receiveid as ins_receiveid,
i.inspectiondate as ins_inspectiondate,
i.inspectionnote as ins_inspectionnote,
i.inspectedby as ins_inspectedby,
i.status as ins_status,
i.createby as ins_createby,
i.createddate as ins_createddate,
i.modifiedby as ins_modifiedby,
i.modifieddate as ins_modifieddate,
i.receiptnumber as ins_receiptnumber,
i.descriptionofinspection as ins_descriptionofinspection,
i.isshippedprovided as ins_isshippedprovided,
i.isshipped as ins_isshipped,
i.shippedcomment as ins_shippedcomment,
i.shippedprovidedcomment as ins_shippedprovidedcomment,
i.conditionofbox as ins_conditionofbox,
i.labelattachedcomment as ins_labelattachedcomment,
ii.id as insi_id,
ii.inspectionid as insi_inspectionid,
ii.productid as insi_productid,
ii.quantitycounted as insi_quantitycounted,
ii.boxcounted as insi_boxcounted,
ii.passquantity as insi_passquantity,
ii.passlocationid as insi_passlocationid,
ii.failquantity as insi_failquantity,
ii.failreason as insi_failreason,
ii.faillocationid as insi_faillocationid,
ii.lotflag as insi_lotflag,
ii.dryiceflag as insi_dryiceflag,
ii.vvmflag as insi_vvmflag,
ii.cccardflag as insi_cccardflag,
ii.electronicdeviceflag as insi_electronicdeviceflag,
ii.createdby as insi_createdby,
ii.createddate as insi_createddate,
ii.modifiedby as insi_modifiedby,
ii.modifieddate as insi_modifieddate,
ii.othermonitor as insi_othermonitor,
ii.icepackflag as insi_icepackflag,
ii.nocoolantflag as insi_nocoolantflag,
r.id as rec_id,
r.purchaseorderid as rec_purchaseorderid,
r.ponumber as rec_ponumber,
r.podate as rec_podate,
r.supplierid as rec_supplierid,
r.asnid as rec_asnid,
r.receivedate as rec_receivedate,
r.blawbnumber as rec_blawbnumber,
r.country as rec_country,
r.flightvesselnumber as rec_flightvesselnumber,
r.portofarrival as rec_portofarrival,
r.expectedarrivaldate as rec_expectedarrivaldate,
r.actualarrivaldate as rec_actualarrivaldate,
r.clearingagent as rec_clearingagent,
r.shippingagent as rec_shippingagent,
r.status as rec_status,
r.note as rec_note,
r.notetosupplier as rec_notetosupplier,
r.description as rec_description,
r.createdby as rec_createdby,
r.createddate as rec_createddate,
r.modifiedby as rec_modifiedby,
r.modifieddate as rec_modifieddate,
r.isforeignprocurement as rec_isforeignprocurement,
r.currencyid as rec_currencyid,
ri.id as reci_id,
ri.receiveid as reci_receiveid,
ri.productid as reci_productid,
ri.expirydate as reci_expirydate,
ri.manufacturingdate as reci_manufacturingdate,
ri.quantitycounted as reci_quantitycounted,
ri.unitprice as reci_unitprice,
ri.boxcounted as reci_boxcounted,
ri.lotflag as reci_lotflag,
ri.createdby as reci_createdby,
ri.createddate as reci_createddate,
ri.modifiedby as reci_modifiedby,
ri.modifieddate as reci_modifieddate,
a.id as asn_id,
a.purchaseorderid as asn_purchaseorderid,
a.ponumber as asn_ponumber,
a.podate as asn_podate,
a.supplierid as asn_supplierid,
a.asnnumber as asn_asnnumber,
a.asndate as asn_asndate,
a.blawbnumber as asn_blawbnumber,
a.flightvesselnumber as asn_flightvesselnumber,
a.portofarrival as asn_portofarrival,
a.expectedarrivaldate as asn_expectedarrivaldate,
a.clearingagent as asn_clearingagent,
a.status as asn_status,
a.note as asn_note,
a.createdby as asn_createdby,
a.createddate as asn_createddate,
a.modifiedby as asn_modifiedby,
a.modifieddate as asn_modifieddate,
a.expecteddeliverydate as asn_expecteddeliverydate,
a.active as asn_active,
a.currencyid as asn_currencyid,
ai.id as asni_id,
ai.asnid as asni_asnid,
ai.productid as asni_productid,
ai.expirydate as asni_expirydate,
ai.manufacturingdate as asni_manufacturingdate,
ai.quantityexpected as asni_quantityexpected,
ai.lotflag as asni_lotflag,
ai.createdby as asni_createdby,
ai.createddate as asni_createddate,
ai.modifiedby as asni_modifiedby,
ai.modifieddate as asni_modifieddate,
ai.unitprice as asni_unitprice,
(select case when (documentType = 1) then 'Yes' else 'No' end as rec_airBill from documents doc
JOIN document_types T on doc.documentType = t.id
 where doc.asnNumber = a.asnNumber limit 1),

(select case when (documentType = 2) then 'Yes' else 'No' end as rec_packList from documents doc
JOIN document_types T on doc.documentType = t.id
 where doc.asnNumber = a.asnNumber limit 1),

 (select case when (documentType = 3) then 'Yes' else 'No' end as rec_invoice from documents doc
JOIN document_types T on doc.documentType = t.id
 where doc.asnNumber = a.asnNumber limit 1),

  (select case when (documentType = 4) then 'Yes' else 'No' end as rec_releaseCertificate
   from documents doc
JOIN document_types T on doc.documentType = t.id
 where doc.asnNumber = a.asnNumber limit 1),
   'Yes' as var_report

   , pp.primaryname product,
   lot.lotNumber ins_lotNumber,

   LOT.EXPIRYDATE ins_expirydate,

pp.primaryname ||' ('|| PACKSIZE ||' '|| u.code ||' )' productDescription,
pp.manufacturer,

case when i.isshipped=true then 'Yes' else 'No' end as isshipped,
case when isshippedprovided= true then 'Yes' else 'No' end as isshippedprovided,
shippedcomment, shippedprovidedcomment


from inspections i
JOIN inspection_line_items ii on i.id = ii.inspectionid
JOIN Receives r on i.receiveId = r.id
JOIN receive_line_items ri ON ri.receiveid = r.id
JOIN asns a ON r.asnId = a.id
JOIN asn_details ai ON a.id = ai.asnId
JOIN products pp ON ii.productId = pp.id
JOIN inspection_lots LOT ON ii.id = LOT.inspectionLINEITEMID
JOIN dosage_units u on pp.dosageunitid = u.id
WHERE I.ID = $P{ORDER_ID}]]>
